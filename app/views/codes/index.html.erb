

<div class="container">
  <p id="notice"><%= notice %></p>
  <h1>Programas Guardados</h1>

  <table class="striped">
    <thead>
      <tr>
        <th>Titulo</th>
        <th>Codigo</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @codes.each do |code| %>
        <tr>
          <td><%= code.titulo %></td>
          <td><%= code.codigo %></td>
          <td><%= link_to 'Ver', code %></td>
          <td><%= link_to 'Editar', edit_code_path(code) %></td>
          <td><%= link_to 'Eliminar', code, method: :delete, data: { confirm: 'El elemento se eliminara!' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <div class="fixed-action-btn horizontal" style="bottom: 45px; right: 24px;">
    <a class="btn-floating btn-large red">
      <i class="large material-icons">add</i>
    </a>
    <ul>
      <li><a href= "<%=new_code_path%>" class="btn-floating red"><i class="material-icons">add</i></a></li>  
      <li><a class="btn-floating blue"><i class="material-icons">attach_file</i></a></li>
    </ul>
  </div>
  <%= link_to '', new_code_path %>
</div>